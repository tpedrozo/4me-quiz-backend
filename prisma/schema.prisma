generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id              String   @id @default(uuid())
  gender          String
  name            String
  email           String   @unique
  phone           String
  zipCode         String
  address         String
  age             String
  complementary   String[]
  goal            String[]
  hairType        String
  hairConsider    String
  hairLength      String
  washFrequency   String
  chemicalProcess String[]
  hairLoss        String
  yourWireAre     String
  dandruff        String
  frizzType       String
  authKey         String
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
}
